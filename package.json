{
	"name": "skycrops-frontend",
	"version": "0.1.0",
	"description": "Modern, responsive Next.js frontend application for SkyCrops with Turkish language support and optimized component architecture",
	"keywords": [
		"nextjs",
		"react",
		"typescript",
		"tailwindcss",
		"shadcn-ui",
		"turkish",
		"agriculture",
		"farming",
		"responsive",
		"accessibility"
	],
	"author": "SkyCrops Development Team",
	"license": "UNLICENSED",
	"private": true,
	"repository": {
		"type": "git",
		"url": "https://github.com/mehmetcanbudak/sc-fe.git"
	},
	"homepage": "https://skycrops.com",
	"bugs": {
		"url": "https://github.com/skycrops/skycrops-final-fe/issues"
	},
	"engines": {
		"node": ">=18.0.0",
		"npm": ">=8.0.0"
	},
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "biome check .",
		"lint:fix": "biome check --apply .",
		"type-check": "tsc --noEmit",
		"clean": "rm -rf .next out dist .turbo",
		"clean:all": "rm -rf .next out dist .turbo node_modules pnpm-lock.yaml",
		"analyze": "ANALYZE=true pnpm build",
		"format": "biome format --write .",
		"format:check": "biome format .",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage",
		"prepare": "echo 'Skipping husky install'",
		"git:status": "git status",
		"git:add": "git add .",
		"git:commit": "git commit -m",
		"git:push": "git push origin main",
		"git:pull": "git pull origin main",
		"git:log": "git log --oneline -10",
		"git:diff": "git diff --cached"
	},
